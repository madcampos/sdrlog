			"type": "image/png",
			"purpose": "maskable"
		},
		{
			"src": "./img/icons/android-chrome-maskable-512x512.png",
			"sizes": "512x512",
			"type": "image/png",
			"purpose": "maskable"
		}
	],
	"protocol_handlers": [
		{
			"protocol": "web+sdrlog",
			"url": "/?search=sku:%s"
		}
	],
